!urxvt.font: 9x15
!urxvt.font: 
urxvt.font: xft:Inconsolata:pixelsize=17

urxvt*internalBorder: 10

urxvt*boldFont: 
urxvt*italicFont: 
urxvt*bolditalicFont: 

urxvt*letterSpace: 0
urxvt*lineSpace: 0

urxvt*scrollBar: false

!real transparency
urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0200/c800
urxvt.saveLines: 9999
urxvt.iso14755: false
!URxvt*altSendsEscape: true


urxvt.cursorUnderline: false

! special
*.foreground:   #ffffff
!*.foreground:   #bcbcbc
*.background:   #101010
!*.cursorColor:  #bcbcbc
*.cursorColor:  #ffffff

